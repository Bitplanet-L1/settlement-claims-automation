# Settlement Claims Automation - Environment Variables
# Copy this to .env and fill in your details

# Email Notifications (Gmail example)
EMAIL_USER=your-email@gmail.com
EMAIL_PASS=your-gmail-app-password
NOTIFY_EMAIL=where-to-receive-alerts@example.com

# Notification Settings
NOTIFY_MIN_AMOUNT=25
NOTIFY_MAX_DAYS_DEADLINE=60

# Automation Settings
AUTO_SCAN_INTERVAL=24
MAX_CLAIMS_PER_RUN=5
